{
  "name": "alirashidnahal/pandaframe",
  "type": "project",
  "description": "A PHP library for data analysis inspired by pandas.",
  "keywords": [
    "pandaframe",
    "library"
  ],
  "license": "GPL-3.0",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0.1"
  },
  "autoload": {
    "psr-4": {
      "PandaFrame\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {},
  "extra": {
    "pandaframe": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
